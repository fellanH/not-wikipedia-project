{
  "name": "not-wikipedia",
  "version": "1.0.0",
  "description": "Wikipedia-style article generation system",
  "scripts": {
    "dashboard": "node local-agent/lib/dashboard/server.js",
    "ralph": "cd local-agent/lib/agent && ./ralph.sh",
    "build": "cd local-agent/lib/mcp && npm run build",
    "publish": "cd local-agent/lib/agent && node -e \"require('../mcp/dist/tools/wiki-git-publish.js').tool.handler({}).then(r=>console.log(r.content[0].text))\"",
    "health": "cd local-agent/lib/agent && node -e \"require('../mcp/dist/tools/wiki-ecosystem.js').tool.handler({}).then(r=>console.log(r.content[0].text))\"",
    "roadmap": "./roadmap-agent.sh",
    "roadmap:single": "./roadmap-agent.sh --single",
    "roadmap:status": "./roadmap-agent.sh --status",
    "roadmap:list": "./roadmap-agent.sh --list",
    "roadmap:dry": "./roadmap-agent.sh --dry-run --single"
  },
  "private": true
}
